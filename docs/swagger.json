{
  "swagger": "2.0",
  "info": {
    "description": "HTTP API for utility management",
    "title": "HTTP-server for \"Modbus emulator\"",
    "version": "1.1.0"
  },
  "basePath": "/modbus-emulator",
  "schemes": [
    "http"
  ],
  "paths": {
    "/settings": {
      "get": {
        "tags": [
          "Settings"
        ],
        "description": "Get list of settings for all working servers",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "settings": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/Server"
                  }
                },
                "id": {
                  "type": "integer"
                }
              }
            }
          }
        }
      }
    }
  },
  "definitions": {
    "Server": {
      "type": "object",
      "properties": {
        "dump_socket": {
          "type": "string"
        },
        "real_socket": {
          "type": "string"
        },
        "protocol": {
          "type": "string"
        },
        "one_time_emulation": {
          "type": "boolean"
        },
        "start_time": {
          "type": "string"
        },
        "end_time": {
          "type": "string"
        },
        "current_time": {
          "type": "string"
        }
      }
    }
  }
